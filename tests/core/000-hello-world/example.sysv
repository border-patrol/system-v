module Swap ( input  wire logic a
            , input  wire logic b
            , output wire logic x
            , output wire logic y);
    assign x = a;
    assign y = b;
endmodule

module DoSwap ();
  wire logic a;
  wire logic b;
  wire logic x;
  wire logic y;

  drive (writeTo a);
  drive (writeTo b);
  catch (readFrom x);
  catch (readFrom y);

  Swap ms ((readFrom a)
          ,(readFrom b)
          ,(writeTo x)
          ,(writeTo y)
          );
endmodule

